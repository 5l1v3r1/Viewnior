#: ../data/viewnior.desktop.in.in.h:1 ../src/vnr-window.c:544
msgid "Elegant Image Viewer"
msgstr "Elegancka przeglądarka obrazów"

#: ../data/viewnior.desktop.in.in.h:2
msgid "Viewnior"
msgstr "Viewnior"

#: ../src/main.c:105 ../src/vnr-window.c:733 ../src/vnr-window.c:1072
msgid "The given locations contain no images."
msgstr "Podana lokalizacja nie zawiera żadnych zdjęć."

#: ../src/uni-scroll-win.c:290
msgid "Open the navigator window"
msgstr "Otwiera okno nawigatora."

#: ../src/vnr-window.c:138
msgid "Show next image after: "
msgstr "Pokaż następny obraz po: "

#: ../src/vnr-window.c:152
msgid " seconds"
msgstr " sekund"

#: ../src/vnr-window.c:676
msgid "If you delete an item, it will be permanently lost."
msgstr "Jeśli usuniesz obraz, zostanie on bezpowrotnie utracony."

#. I18N: The '%s' is replaced with the name of the file to be deleted.
#: ../src/vnr-window.c:679
#, c-format
msgid ""
"Are you sure you want to\n"
"permanently delete \"%s\"?"
msgstr ""
"Czy jesteś pewien że chcesz\n"
"permanentnie usunąć \"%s\"?"

#: ../src/vnr-window.c:706
#, c-format
msgid "Error deleting image: %s"
msgstr "Bład przy usuwaniu obrazu: %s"

#: ../src/vnr-window.c:764
msgid "_File"
msgstr "_Plik"

#: ../src/vnr-window.c:765
msgid "_View"
msgstr "_Widok"

#: ../src/vnr-window.c:766
msgid "_Go"
msgstr "_Idź"

#: ../src/vnr-window.c:767
msgid "_Help"
msgstr "_Pomoc"

#: ../src/vnr-window.c:769
msgid "Open _Image..."
msgstr "_Otwórz obraz..."

#: ../src/vnr-window.c:770
msgid "Open an Image"
msgstr "Otwiera obraz"

#: ../src/vnr-window.c:772
msgid "Open _Folder..."
msgstr "Otwórz _folder..."

#: ../src/vnr-window.c:773
msgid "Open a Folder"
msgstr "Otwiera folder"

#: ../src/vnr-window.c:775
msgid "_Close"
msgstr "_Zamknij"

#: ../src/vnr-window.c:776
msgid "Close window"
msgstr "Zamyka aktualne okno"

#: ../src/vnr-window.c:778
msgid "_About"
msgstr "O programie..."

#: ../src/vnr-window.c:779
msgid "About this application"
msgstr "Pokazuje okno z informacją o tym programie"

#: ../src/vnr-window.c:785
msgid "Set as _Wallpaper"
msgstr "Ustaw jako _tapetę"

#: ../src/vnr-window.c:786
msgid "Set the selected image as the desktop background"
msgstr "Ustawia obecnie załadowany obraz jako tło pulpitu"

#: ../src/vnr-window.c:789 ../src/vnr-window.c:792
msgid "_Delete"
msgstr "_Usuń"

#: ../src/vnr-window.c:790 ../src/vnr-window.c:793
msgid "Delete the current file"
msgstr "Usuwa aktualny plk"

#: ../src/vnr-window.c:795 ../src/vnr-window.c:810 ../src/vnr-window.c:813
msgid "_Zoom In"
msgstr "Powiększ"

#: ../src/vnr-window.c:796
msgid "Enlarge the image"
msgstr "Powiększa obraz"

#: ../src/vnr-window.c:798 ../src/vnr-window.c:816
msgid "Zoom _Out"
msgstr "Pomnniejsz"

#: ../src/vnr-window.c:799 ../src/vnr-window.c:811 ../src/vnr-window.c:814
#: ../src/vnr-window.c:817
msgid "Shrink the image"
msgstr "Pomniejsza obraz"

#: ../src/vnr-window.c:801
msgid "_Normal Size"
msgstr "_Normalny rozmiar"

#: ../src/vnr-window.c:802
msgid "Show the image at its normal size"
msgstr "Pokazuje obraz w normalnym rozmiarze"

#: ../src/vnr-window.c:804
msgid "Best _Fit"
msgstr "Najlepsze dopasowanie"

#: ../src/vnr-window.c:805
msgid "Fit the image to the window"
msgstr "Skaluj obraz do okna"

#: ../src/vnr-window.c:807
msgid "_Resize Window"
msgstr "Zmień rozmiar okna"

#: ../src/vnr-window.c:808
msgid "Resize the window to fit the image"
msgstr "Zmienia rozmiar okna dopasowując do rozmiaru obrazu"

#: ../src/vnr-window.c:822
msgid "Full _Screen"
msgstr "Pełny _ekran"

#: ../src/vnr-window.c:823
msgid "Show in fullscreen mode"
msgstr "Pokazuje obraz w trybie pełnoekranowym"

#: ../src/vnr-window.c:828
msgid "_Previous Image"
msgstr "_Poprzedni obraz"

#: ../src/vnr-window.c:829
msgid "Go to the previous image of the collection"
msgstr "Przechodzi do poprzedniego obrazu w kolekcji"

#: ../src/vnr-window.c:831
msgid "_Next Image"
msgstr "_Następny obraz"

#: ../src/vnr-window.c:832
msgid "Go to the next image of the collection"
msgstr "Przechodzi do następnego obrazu w kolekcji"

#: ../src/vnr-window.c:834
msgid "_First Image"
msgstr "_Pierwszy obraz"

#: ../src/vnr-window.c:835
msgid "Go to the first image of the collection"
msgstr "Pokazuje pierwszy obraz w kolekcji"

#: ../src/vnr-window.c:837
msgid "_Last Image"
msgstr "_Ostatni obraz"

#: ../src/vnr-window.c:838
msgid "Go to the last image of the collection"
msgstr "Pokazuje ostatni obraz w kolekcji"
