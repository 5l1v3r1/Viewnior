#: ../data/viewnior.desktop.in.in.h:1 ../src/vnr-window.c:257
msgid "Elegant Image Viewer"
msgstr "Елегантна програма за разглеждане на картинки"

#: ../data/viewnior.desktop.in.in.h:2
msgid "Viewnior"
msgstr "Viewnior"

#: ../src/main.c:109 ../src/vnr-window.c:396 ../src/vnr-window.c:702
msgid "The given locations contain no images."
msgstr "Не са открити изображения"

#: ../src/uni-scroll-win.c:290
msgid "Open the navigator window"
msgstr "Отваряне прозореца за навигация"

#: ../src/vnr-window.c:343
msgid "If you delete an item, it will be permanently lost."
msgstr "Ако изтриете файл, той се загубва завинаги."

#. I18N: The '%s' is replaced with the name of the file to be deleted.
#: ../src/vnr-window.c:346
#, c-format
msgid ""
"Are you sure you want to\n"
"permanently delete \"%s\"?"
msgstr ""
"Сигурни ли сте, че искате да\n"
"изтриете окончателно \"%s\"?"

#: ../src/vnr-window.c:373
#, c-format
msgid "Error deleting image: %s"
msgstr "Грешка при изтриване: %s"

#: ../src/vnr-window.c:423
msgid "_File"
msgstr "Файл"

#: ../src/vnr-window.c:424
msgid "_View"
msgstr "Изглед"

#: ../src/vnr-window.c:425
msgid "_Go"
msgstr "Начало"

#: ../src/vnr-window.c:426
msgid "_Help"
msgstr "Помощ"

#: ../src/vnr-window.c:428
msgid "_Open"
msgstr "Отваряне"

#: ../src/vnr-window.c:429
msgid "Open a File"
msgstr "Отваряне на файл"

#: ../src/vnr-window.c:431
msgid "_Close"
