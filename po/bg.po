msgid "Set as _Wallpaper"
msgstr "Задаване като фон"

#: ../src/vnr-window.c:442
msgid "Set the selected image as the desktop background"
msgstr "Задаване на изображението като фон"

#: ../src/vnr-window.c:445 ../src/vnr-window.c:448
msgid "_Delete"
msgstr "Изтриване"

#: ../src/vnr-window.c:446 ../src/vnr-window.c:449
msgid "Delete the current file"
msgstr "Изтриване на текущия файл"

#: ../src/vnr-window.c:451 ../src/vnr-window.c:463 ../src/vnr-window.c:466
msgid "_Zoom In"
msgstr "Увеличаване"

#: ../src/vnr-window.c:452
msgid "Enlarge the image"
msgstr "Увеличаване на изображението"

#: ../src/vnr-window.c:454 ../src/vnr-window.c:469
msgid "Zoom _Out"
msgstr "Намаляване"

#: ../src/vnr-window.c:455 ../src/vnr-window.c:464 ../src/vnr-window.c:467
#: ../src/vnr-window.c:470
msgid "Shrink the image"
