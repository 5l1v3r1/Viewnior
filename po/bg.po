#: ../data/viewnior.desktop.in.in.h:1 ../src/vnr-window.c:546
msgid "Elegant Image Viewer"
msgstr "Елегантна програма за разглеждане на картинки"

#: ../data/viewnior.desktop.in.in.h:2
msgid "Viewnior"
msgstr "Viewnior"

#: ../src/main.c:105 ../src/vnr-window.c:735 ../src/vnr-window.c:1074
msgid "The given locations contain no images."
msgstr "Не са открити изображения"

#: ../src/uni-scroll-win.c:290
msgid "Open the navigator window"
msgstr "Отваряне прозореца за навигация"

#: ../src/vnr-window.c:138
msgid "Show next image after: "
msgstr "Смяна на изображението след: "

#: ../src/vnr-window.c:152
msgid " seconds"
msgstr " секунди"

#: ../src/vnr-window.c:678
msgid "If you delete an item, it will be permanently lost."
msgstr "Ако изтриете файл, той се загубва завинаги."

#. I18N: The '%s' is replaced with the name of the file to be deleted.
#: ../src/vnr-window.c:681
#, c-format
msgid ""
"Are you sure you want to\n"
"permanently delete \"%s\"?"
msgstr ""
"Сигурни ли сте, че искате да\n"
"изтриете окончателно \"%s\"?"

#: ../src/vnr-window.c:708
#, c-format
msgid "Error deleting image: %s"
msgstr "Грешка при изтриване: %s"

#: ../src/vnr-window.c:766
msgid "_File"
msgstr "Файл"

#: ../src/vnr-window.c:767
msgid "_View"
msgstr "Изглед"

#: ../src/vnr-window.c:768
msgid "_Go"
msgstr "Начало"

#: ../src/vnr-window.c:769
msgid "_Help"
msgstr "Помощ"

#: ../src/vnr-window.c:771
msgid "Open _Image..."
msgstr "Отваряне на изображение"

#: ../src/vnr-window.c:772
msgid "Open an Image"
msgstr "Отваряне на изображение"

#: ../src/vnr-window.c:774
msgid "Open _Folder..."
msgstr "Отваряне на папка"

#: ../src/vnr-window.c:775
msgid "Open a Folder"
msgstr "Отваряне на папка"

#: ../src/vnr-window.c:777
msgid "_Close"
msgstr "Затваряне"

#: ../src/vnr-window.c:778
msgid "Close window"
